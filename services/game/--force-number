function _encodePaddle(message, bb) {
  // optional int32 id = 1;
  let $id = message.id;
  if ($id !== undefined) {
    writeVarint32(bb, 8);
    writeVarint64(bb, intToLong($id));
  }

  // optional int32 playerId = 2;
  let $playerId = message.playerId;
  if ($playerId !== undefined) {
    writeVarint32(bb, 16);
    writeVarint64(bb, intToLong($playerId));
  }

  // optional int32 move = 3;
  let $move = message.move;
  if ($move !== undefined) {
    writeVarint32(bb, 24);
    writeVarint64(bb, intToLong($move));
  }

  // optional float offset = 4;
  let $offset = message.offset;
  if ($offset !== undefined) {
    writeVarint32(bb, 37);
    writeFloat(bb, $offset);
  }

  // optional bool dead = 5;
  let $dead = message.dead;
  if ($dead !== undefined) {
    writeVarint32(bb, 40);
    writeByte(bb, $dead ? 1 : 0);
  }
}

