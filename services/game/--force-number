export function encodeGameEvent(message) {
  let bb = popByteBuffer();
  _encodeGameEvent(message, bb);
  return toUint8Array(bb);
}

function _encodeGameEvent(message, bb) {
  // optional string type = 1;
  let $type = message.type;
  if ($type !== undefined) {
    writeVarint32(bb, 10);
    writeString(bb, $type);
  }

  // optional string phase = 2;
  let $phase = message.phase;
  if ($phase !== undefined) {
    writeVarint32(bb, 18);
    writeString(bb, $phase);
  }

  // optional int32 remainingPlayers = 3;
  let $remainingPlayers = message.remainingPlayers;
  if ($remainingPlayers !== undefined) {
    writeVarint32(bb, 24);
    writeVarint64(bb, intToLong($remainingPlayers));
  }

  // optional int64 timestamp = 4;
  let $timestamp = message.timestamp;
  if ($timestamp !== undefined) {
    writeVarint32(bb, 32);
    writeVarint64(bb, $timestamp);
  }

  // repeated int32 activePlayers = 5;
  let array$activePlayers = message.activePlayers;
  if (array$activePlayers !== undefined) {
    let packed = popByteBuffer();
    for (let value of array$activePlayers) {
      writeVarint64(packed, intToLong(value));
    }
    writeVarint32(bb, 42);
    writeVarint32(bb, packed.offset);
    writeByteBuffer(bb, packed);
    pushByteBuffer(packed);
  }

  // optional map<int32, int32> playerMapping = 6;
  let map$playerMapping = message.playerMapping;
  if (map$playerMapping !== undefined) {
    for (let key in map$playerMapping) {
      let nested = popByteBuffer();
      let value = map$playerMapping[key];
      writeVarint32(nested, 8);
      writeVarint64(nested, intToLong(+key));
      writeVarint32(nested, 16);
      writeVarint64(nested, intToLong(value));
      writeVarint32(bb, 50);
      writeVarint32(bb, nested.offset);
      writeByteBuffer(bb, nested);
      pushByteBuffer(nested);
    }
  }
}

export function decodeGameEvent(binary) {
  return _decodeGameEvent(wrapByteBuffer(binary));
}

function _decodeGameEvent(bb) {
  let message = {};

  end_of_message: while (!isAtEnd(bb)) {
    let tag = readVarint32(bb);

    switch (tag >>> 3) {
      case 0:
        break end_of_message;

      // optional string type = 1;
      case 1: {
        message.type = readString(bb, readVarint32(bb));
        break;
      }

      // optional string phase = 2;
      case 2: {
        message.phase = readString(bb, readVarint32(bb));
        break;
      }

      // optional int32 remainingPlayers = 3;
      case 3: {
        message.remainingPlayers = readVarint32(bb);
        break;
      }

      // optional int64 timestamp = 4;
      case 4: {
        message.timestamp = readVarint64(bb, /* unsigned */ false);
        break;
      }

      // repeated int32 activePlayers = 5;
      case 5: {
        let values = message.activePlayers || (message.activePlayers = []);
        if ((tag & 7) === 2) {
          let outerLimit = pushTemporaryLength(bb);
          while (!isAtEnd(bb)) {
            values.push(readVarint32(bb));
          }
          bb.limit = outerLimit;
        } else {
          values.push(readVarint32(bb));
        }
        break;
      }

      // optional map<int32, int32> playerMapping = 6;
      case 6: {
        let values = message.playerMapping || (message.playerMapping = {});
        let outerLimit = pushTemporaryLength(bb);
        let key;
        let value;
        end_of_entry: while (!isAtEnd(bb)) {
          let tag = readVarint32(bb);
          switch (tag >>> 3) {
            case 0:
              break end_of_entry;
            case 1: {
              key = readVarint32(bb);
              break;
            }
            case 2: {
              value = readVarint32(bb);
              break;
            }
            default:
              skipUnknownField(bb, tag & 7);
          }
        }
        if (key === undefined || value === undefined)
          throw new Error("Invalid data for map: playerMapping");
        values[key] = value;
        bb.limit = outerLimit;
        break;
      }

      default:
        skipUnknownField(bb, tag & 7);
    }
  }

  return message;
}

export function encodeGameStateInfo(message) {
  let bb = popByteBuffer();
  _encodeGameStateInfo(message, bb);
  return toUint8Array(bb);
}

function _encodeGameStateInfo(message, bb) {
  // repeated int32 activePlayers = 1;
  let array$activePlayers = message.activePlayers;
  if (array$activePlayers !== undefined) {
    let packed = popByteBuffer();
    for (let value of array$activePlayers) {
      writeVarint64(packed, intToLong(value));
    }
    writeVarint32(bb, 10);
    writeVarint32(bb, packed.offset);
    writeByteBuffer(bb, packed);
    pushByteBuffer(packed);
  }

  // repeated int32 eliminatedPlayers = 2;
  let array$eliminatedPlayers = message.eliminatedPlayers;
  if (array$eliminatedPlayers !== undefined) {
    let packed = popByteBuffer();
    for (let value of array$eliminatedPlayers) {
      writeVarint64(packed, intToLong(value));
    }
    writeVarint32(bb, 18);
    writeVarint32(bb, packed.offset);
    writeByteBuffer(bb, packed);
    pushByteBuffer(packed);
  }

  // optional string currentPhase = 3;
  let $currentPhase = message.currentPhase;
  if ($currentPhase !== undefined) {
    writeVarint32(bb, 26);
    writeString(bb, $currentPhase);
  }

  // optional bool isRebuilding = 4;
  let $isRebuilding = message.isRebuilding;
  if ($isRebuilding !== undefined) {
    writeVarint32(bb, 32);
    writeByte(bb, $isRebuilding ? 1 : 0);
  }

  // optional int64 rebuildTimeRemaining = 5;
  let $rebuildTimeRemaining = message.rebuildTimeRemaining;
  if ($rebuildTimeRemaining !== undefined) {
    writeVarint32(bb, 40);
    writeVarint64(bb, $rebuildTimeRemaining);
  }

  // optional map<int32, int32> playerMapping = 6;
  let map$playerMapping = message.playerMapping;
  if (map$playerMapping !== undefined) {
    for (let key in map$playerMapping) {
      let nested = popByteBuffer();
      let value = map$playerMapping[key];
      writeVarint32(nested, 8);
      writeVarint64(nested, intToLong(+key));
      writeVarint32(nested, 16);
      writeVarint64(nested, intToLong(value));
      writeVarint32(bb, 50);
      writeVarint32(bb, nested.offset);
      writeByteBuffer(bb, nested);
      pushByteBuffer(nested);
    }
  }

  // optional bool isGameOver = 7;
  let $isGameOver = message.isGameOver;
  if ($isGameOver !== undefined) {
    writeVarint32(bb, 56);
    writeByte(bb, $isGameOver ? 1 : 0);
  }

  // optional int32 winner = 8;
  let $winner = message.winner;
  if ($winner !== undefined) {
    writeVarint32(bb, 64);
    writeVarint64(bb, intToLong($winner));
  }
}

export function decodeGameStateInfo(binary) {
  return _decodeGameStateInfo(wrapByteBuffer(binary));
}

function _decodeGameStateInfo(bb) {
  let message = {};

  end_of_message: while (!isAtEnd(bb)) {
    let tag = readVarint32(bb);

    switch (tag >>> 3) {
      case 0:
        break end_of_message;

      // repeated int32 activePlayers = 1;
      case 1: {
        let values = message.activePlayers || (message.activePlayers = []);
        if ((tag & 7) === 2) {
          let outerLimit = pushTemporaryLength(bb);
          while (!isAtEnd(bb)) {
            values.push(readVarint32(bb));
          }
          bb.limit = outerLimit;
        } else {
          values.push(readVarint32(bb));
        }
        break;
      }

      // repeated int32 eliminatedPlayers = 2;
      case 2: {
        let values = message.eliminatedPlayers || (message.eliminatedPlayers = []);
        if ((tag & 7) === 2) {
          let outerLimit = pushTemporaryLength(bb);
          while (!isAtEnd(bb)) {
            values.push(readVarint32(bb));
          }
          bb.limit = outerLimit;
        } else {
          values.push(readVarint32(bb));
        }
        break;
      }

      // optional string currentPhase = 3;
      case 3: {
        message.currentPhase = readString(bb, readVarint32(bb));
        break;
      }

      // optional bool isRebuilding = 4;
      case 4: {
        message.isRebuilding = !!readByte(bb);
        break;
      }

      // optional int64 rebuildTimeRemaining = 5;
      case 5: {
        message.rebuildTimeRemaining = readVarint64(bb, /* unsigned */ false);
        break;
      }

      // optional map<int32, int32> playerMapping = 6;
      case 6: {
        let values = message.playerMapping || (message.playerMapping = {});
        let outerLimit = pushTemporaryLength(bb);
        let key;
        let value;
        end_of_entry: while (!isAtEnd(bb)) {
          let tag = readVarint32(bb);
          switch (tag >>> 3) {
            case 0:
              break end_of_entry;
            case 1: {
              key = readVarint32(bb);
              break;
            }
            case 2: {
              value = readVarint32(bb);
              break;
            }
            default:
              skipUnknownField(bb, tag & 7);
          }
        }
        if (key === undefined || value === undefined)
          throw new Error("Invalid data for map: playerMapping");
        values[key] = value;
        bb.limit = outerLimit;
        break;
      }

      // optional bool isGameOver = 7;
      case 7: {
        message.isGameOver = !!readByte(bb);
        break;
      }

      // optional int32 winner = 8;
      case 8: {
        message.winner = readVarint32(bb);
        break;
      }

      default:
        skipUnknownField(bb, tag & 7);
    }
  }

  return message;
}

export function encodeMatchState(message) {
  let bb = popByteBuffer();
  _encodeMatchState(message, bb);
  return toUint8Array(bb);
}

function _encodeMatchState(message, bb) {
  // optional string gameId = 1;
  let $gameId = message.gameId;
  if ($gameId !== undefined) {
    writeVarint32(bb, 10);
    writeString(bb, $gameId);
  }

  // optional int64 tick = 2;
  let $tick = message.tick;
  if ($tick !== undefined) {
    writeVarint32(bb, 16);
    writeVarint64(bb, $tick);
  }

  // repeated Ball balls = 3;
  let array$balls = message.balls;
  if (array$balls !== undefined) {
    for (let value of array$balls) {
      writeVarint32(bb, 26);
      let nested = popByteBuffer();
      _encodeBall(value, nested);
      writeVarint32(bb, nested.limit);
      writeByteBuffer(bb, nested);
      pushByteBuffer(nested);
    }
  }

  // repeated Paddle paddles = 4;
  let array$paddles = message.paddles;
  if (array$paddles !== undefined) {
    for (let value of array$paddles) {
      writeVarint32(bb, 34);
      let nested = popByteBuffer();
      _encodePaddle(value, nested);
      writeVarint32(bb, nested.limit);
      writeByteBuffer(bb, nested);
      pushByteBuffer(nested);
    }
  }

  // repeated int32 score = 5;
  let array$score = message.score;
  if (array$score !== undefined) {
    let packed = popByteBuffer();
    for (let value of array$score) {
      writeVarint64(packed, intToLong(value));
    }
    writeVarint32(bb, 42);
    writeVarint32(bb, packed.offset);
    writeByteBuffer(bb, packed);
    pushByteBuffer(packed);
  }

  // repeated int32 ranks = 6;
  let array$ranks = message.ranks;
  if (array$ranks !== undefined) {
    let packed = popByteBuffer();
    for (let value of array$ranks) {
      writeVarint64(packed, intToLong(value));
    }
    writeVarint32(bb, 50);
    writeVarint32(bb, packed.offset);
    writeByteBuffer(bb, packed);
    pushByteBuffer(packed);
  }

  // optional int32 stage = 7;
  let $stage = message.stage;
  if ($stage !== undefined) {
    writeVarint32(bb, 56);
    writeVarint64(bb, intToLong($stage));
  }

  // repeated GameEvent events = 8;
  let array$events = message.events;
  if (array$events !== undefined) {
    for (let value of array$events) {
      writeVarint32(bb, 66);
      let nested = popByteBuffer();
      _encodeGameEvent(value, nested);
      writeVarint32(bb, nested.limit);
      writeByteBuffer(bb, nested);
      pushByteBuffer(nested);
    }
  }

  // optional GameStateInfo gameState = 9;
  let $gameState = message.gameState;
  if ($gameState !== undefined) {
    writeVarint32(bb, 74);
    let nested = popByteBuffer();
    _encodeGameStateInfo($gameState, nested);
    writeVarint32(bb, nested.limit);
    writeByteBuffer(bb, nested);
    pushByteBuffer(nested);
  }
}

export function decodeMatchState(binary) {
  return _decodeMatchState(wrapByteBuffer(binary));
}

function _decodeMatchState(bb) {
  let message = {};

  end_of_message: while (!isAtEnd(bb)) {
    let tag = readVarint32(bb);

    switch (tag >>> 3) {
      case 0:
        break end_of_message;

      // optional string gameId = 1;
      case 1: {
        message.gameId = readString(bb, readVarint32(bb));
        break;
      }

      // optional int64 tick = 2;
      case 2: {
        message.tick = readVarint64(bb, /* unsigned */ false);
        break;
      }

      // repeated Ball balls = 3;
      case 3: {
        let limit = pushTemporaryLength(bb);
        let values = message.balls || (message.balls = []);
        values.push(_decodeBall(bb));
        bb.limit = limit;
        break;
      }

      // repeated Paddle paddles = 4;
      case 4: {
        let limit = pushTemporaryLength(bb);
        let values = message.paddles || (message.paddles = []);
        values.push(_decodePaddle(bb));
        bb.limit = limit;
        break;
      }

      // repeated int32 score = 5;
      case 5: {
        let values = message.score || (message.score = []);
        if ((tag & 7) === 2) {
          let outerLimit = pushTemporaryLength(bb);
          while (!isAtEnd(bb)) {
            values.push(readVarint32(bb));
          }
          bb.limit = outerLimit;
        } else {
          values.push(readVarint32(bb));
        }
        break;
      }

      // repeated int32 ranks = 6;
      case 6: {
        let values = message.ranks || (message.ranks = []);
        if ((tag & 7) === 2) {
          let outerLimit = pushTemporaryLength(bb);
          while (!isAtEnd(bb)) {
            values.push(readVarint32(bb));
          }
          bb.limit = outerLimit;
        } else {
          values.push(readVarint32(bb));
        }
        break;
      }

      // optional int32 stage = 7;
      case 7: {
        message.stage = readVarint32(bb);
        break;
      }

      // repeated GameEvent events = 8;
      case 8: {
        let limit = pushTemporaryLength(bb);
        let values = message.events || (message.events = []);
        values.push(_decodeGameEvent(bb));
        bb.limit = limit;
        break;
      }

      // optional GameStateInfo gameState = 9;
      case 9: {
        let limit = pushTemporaryLength(bb);
        message.gameState = _decodeGameStateInfo(bb);
        bb.limit = limit;
        break;
      }

      default:
        skipUnknownField(bb, tag & 7);
    }
  }

  return message;
}

